{
  "name": "fridgeezy-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "server": "ts-node src/server/index.ts",
    "supabase:reset": "supabase db reset",
    "supabase:reset:linked": "cd src/supabase && supabase db reset --linked --yes",
    "supabase:types": "cd src/supabase && supabase gen types --project-id qkxznjwlybjqpcauaisz > dist/database.types.ts",
    "generate:tag-embeddings": "ts-node src/supabase/scripts/generate-tag-embeddings.ts"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@openai/agents": "^0.3.7",
    "@supabase/supabase-js": "^2.89.0",
    "@types/cors": "^2.8.19",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "openai": "^6.15.0",
    "supabase": "^2.67.3",
    "ts-case-convert": "^2.1.0",
    "ts-node": "^10.9.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.50.0"
  },
  "private": true
}
